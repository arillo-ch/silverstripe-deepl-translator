{
    "name": "arillo/silverstripe-deepl-translator",
    "type": "silverstripe-vendormodule",
    "description": "Adds deepl translations to silverstripe CMS",
    "license": "BSD-3-Clause",
    "keywords": [
        "silverstripe",
        "translation",
        "deepl"
    ],
    "authors": [
        {
            "name": "Arillo Team",
            "email": "hello@arillo.ch"
        }
    ],
    "homepage": "https://github.com/arillo-ch/silverstripe-deepl-translator",
    "support": {
        "issues": "https://github.com/arillo-ch/silverstripe-deepl-translator/issues"
    },
    "require": {
        "tractorcow/silverstripe-fluent": "^5 || ^6",
        "deeplcom/deepl-php": "^1.3.0",
        "amphp/parallel-functions": "^1.1",
        "arillo/silverstripe-cms-alpine": "dev-main"
    },
    "repositories": [
        {
            "type": "git",
            "url": "git@github.com:arillo-ch/silverstripe-cms-alpine.git"
        }
    ],
    "autoload": {
        "psr-4": {
            "Arillo\\Deepl\\": "src/"
        }
    },
    "extra": {
        "expose": [
            "client"
        ]
    }
}